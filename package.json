{
  "name": "start",
  "version": "1.0.0",
  "private": true,
  "description": "taro start",
  "templateInfo": {
    "name": "mobx",
    "typescript": true,
    "css": "sass"
  },
  "main": "index.js",
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "dev": "npm run dev:weapp & npm run lint:watch",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "lint": "./node_modules/.bin/eslint --ext .ts,.tsx src/",
    "lint:fix": "./node_modules/.bin/eslint --ext .ts,.tsx src/ --fix",
    "lint:watch": "nodemon --watch src --ext .ts,.tsx --exec npm run lint",
    "lintstyle": "stylelint **/*.{css,scss} --syntax scss",
    "lintstyle:fix": "stylelint **/*.{css,scss} --syntax scss --fix",
    "lintstyle:watch": "nodemon --watch src --ext .css,.scss --exec npm run lintstyle",
    "lint:all": "run-p lint:watch lintstyle:watch",
    "commit": "git cz",
    "precommit": "lint-staged",
    "create:page": "node scripts/page",
    "create:comp": "node scripts/component"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tarojs/async-await": "^1.3.21",
    "@tarojs/components": "1.3.20",
    "@tarojs/mobx": "1.3.20",
    "@tarojs/mobx-h5": "1.3.20",
    "@tarojs/mobx-rn": "1.3.20",
    "@tarojs/rn-runner": "1.3.20",
    "@tarojs/router": "1.3.20",
    "@tarojs/taro": "1.3.20",
    "@tarojs/taro-alipay": "1.3.20",
    "@tarojs/taro-h5": "1.3.20",
    "@tarojs/taro-swan": "1.3.20",
    "@tarojs/taro-tt": "1.3.20",
    "@tarojs/taro-weapp": "1.3.20",
    "classnames": "^2.2.6",
    "mobx": "4.8.0",
    "nerv-devtools": "^1.4.0",
    "nervjs": "^1.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@tarojs/plugin-babel": "1.3.20",
    "@tarojs/plugin-csso": "1.3.20",
    "@tarojs/plugin-sass": "1.3.20",
    "@tarojs/plugin-uglifyjs": "1.3.20",
    "@tarojs/webpack-runner": "1.3.20",
    "@types/react": "16.3.14",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^1.6.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-jsx-stylesheet": "^0.6.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-taro": "1.3.20",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.6.1",
    "eslint-plugin-taro": "1.3.20",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "stylelint": "^9.3.0",
    "stylelint-config-taro-rn": "1.3.20",
    "stylelint-taro-rn": "1.3.20",
    "typescript": "^3.0.1"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run lint:fix",
      "git add"
    ],
    "src/**/*.{css,scss}": [
      "npm run lintstyle:fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "nodemonConfig": {
    "ignore": [
      "dist/*",
      "config/*"
    ]
  }
}
